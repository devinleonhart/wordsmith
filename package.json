{
  "name": "wordsmith-discord-bot",
  "version": "1.0.0",
  "scripts": {
    "build": "npx tsc && npx babel src --out-dir dist --extensions .ts",
    "debug": "npx nodemon --inspect-brk ./src/app.ts",
    "lint": "npx eslint ./src",
    "serve": "npx nodemon ./src/app.ts",
    "start": "node ./dist/app.js"
  },
  "dependencies": {
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "fuzzy-dice": "^2.0.0",
    "redis": "^3.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/node": "^16.0.0",
    "@types/redis": "^2.8.30",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint": "^7.30.0",
    "nodemon": "^2.0.9",
    "prettier": "2.3.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
