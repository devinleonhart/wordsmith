{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "esnext",
    ],
    "outDir": "./dist",
    "rootDir": "./",
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "suppressImplicitAnyIndexErrors": true,
  }
}
